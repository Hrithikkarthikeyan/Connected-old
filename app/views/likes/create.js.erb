<% if !(@post == nil)%>
document.querySelector('#like-icon-post-<%= @post.id.to_s%>').
  innerHTML = '<%= link_to "Liked", like_path(@like), method: :delete, remote: true, class: "like p-2 cursor like-active fa fa-thumbs-o-up"%>';
document.querySelector('#like-text-post-<%= @post.id.to_s %>').
  innerHTML = '<%= j render "posts/like_text", {likes: @post.likes} %>';
<% end %>
console.log("create like success");